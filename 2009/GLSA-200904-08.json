  ],
  "Access": "remote",
  "Affected": {
    "Package": [
      {
        "Name": "dev-libs/openssl",
        "Auto": "yes",
        "Arch": "*",
        "Unaffected": [
          {
            "Text": "0.9.8k",
            "Range": "ge",
            "Slot": ""
