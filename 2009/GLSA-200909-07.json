{
  "ID": "200909-07",
  "Title": "TkMan: Insecure temporary file usage",
  "Synopsis": "\n    An insecure temporary file usage has been reported in TkMan, allowing for\n    symlink attacks.\n  ",
  "Product": {
    "Text": "tkman",
    "Type": "ebuild"
  },
  "Announced": "2009-09-09",
