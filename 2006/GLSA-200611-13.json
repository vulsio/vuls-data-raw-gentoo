{
  "ID": "200611-13",
  "Title": "Avahi: \"netlink\" message vulnerability",
  "Synopsis": "\n    Avahi fails to verify the origin of netlink messages, which could allow\n    local users to spoof network changes.\n  ",
  "Product": {
    "Text": "avahi",
    "Type": "ebuild"
  },
