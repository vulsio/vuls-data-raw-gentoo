	"Background": {
		"Text": "\n    <p>JasPer is a software-based implementation of the codec specified in the\n      JPEG-2000 Part-1 standard.\n    </p>\n  "
	},
	"Description": {
		"Text": "\n    <p>Multiple vulnerabilities have been discovered in JasPer. Please review\n      the CVE identifiers referenced below for details.\n    </p>\n  "
	},
	"Impact": {
		"Type": "normal",
		"Text": "\n    <p>Please review the referenced CVE identifiers for details.</p>\n  "
	},
	"Workaround": {
		"Text": "\n    <p>There is no known workaround at this time.</p>\n  "
	},
	"Resolution": {
		"Text": "\n    \n    \n    \n  ",
		"P": [
			{
				"Text": "JasPer is no longer maintained upstream and contains many\n      vulnerabilities which remain unaddressed. Gentoo users are advised to\n      unmerge this package.\n    ",
				"URI": {
					"Text": "",
					"Link": ""
				},
				"Br": null,
				"I": null,
				"Code": "",
				"P": ""
			}
		],
		"Code": [
			"\n      # emerge --unmerge media-libs/jasper\n    "
		]
	},
