  "Affected": {
    "Package": [
      {
        "Name": "net-mail/cmd5checkpw",
        "Auto": "yes",
        "Arch": "*",
        "Unaffected": [
          {
            "Text": "0.22-r2",
            "Range": "ge",
            "Slot": ""
          }
        ],
        "Vulnerable": [
          {
            "Text": "0.22-r1",
            "Range": "le",
