{
  "ID": "200502-30",
  "Title": "cmd5checkpw: Local password leak vulnerability",
  "Synopsis": "\n    cmd5checkpw contains a flaw allowing local users to access other users\n    cmd5checkpw passwords.\n  ",
  "Product": {
    "Text": "cmd5checkpw",
    "Type": "ebuild"
  },
  "Announced": "2005-02-25",
  "Revised": {
