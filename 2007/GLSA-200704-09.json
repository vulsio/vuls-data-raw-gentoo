{
  "ID": "200704-09",
  "Title": "xine-lib: Heap-based buffer overflow",
  "Synopsis": "\n    xine-lib is vulnerable to a heap-based buffer overflow.\n  ",
  "Product": {
    "Text": "xine-lib",
    "Type": "ebuild"
  },
  "Announced": "2007-04-14",
